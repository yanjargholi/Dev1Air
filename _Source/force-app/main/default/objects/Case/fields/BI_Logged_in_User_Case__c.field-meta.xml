<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BI_Logged_in_User_Case__c</fullName>
    <description>Checks if the logged in user is the user for a case so that we can create report and list views based on custom user field</description>
    <externalId>false</externalId>
    <formula>IF( BI_User__r.Id  =  $User.Id , 1,0)</formula>
    <label>Logged in User Case</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
